      ],
      "@jurgen-app/shared/data-access": [
        "libs/shared/data-access/src/index.ts"
      ],
      "@jurgen-app/shared/ui-charts": ["libs/shared/ui-charts/src/index.ts"],
      "@jurgen-app/shared/ui-dashboard": [
        "libs/shared/ui-dashboard/src/index.ts"
      ],
      "@jurgen-app/shared/ui-data-table": [
        "libs/shared/ui-data-table/src/index.ts"
      ],
      "@jurgen-app/shared/util-auth": ["libs/shared/util-auth/src/index.ts"],
